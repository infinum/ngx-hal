{
	"$schema": "http://json.schemastore.org/prettierrc",
	"printWidth": 100,
	"endOfLine": "lf",
	"useTabs": true,
	"arrowParens": "always",
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"singleQuote": true,
	"semi": true,
	"trailingComma": "all",
	"plugins": ["prettier-plugin-organize-imports"]
}
